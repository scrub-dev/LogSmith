<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="colour-background-light">#424549</Color>
    <Color x:Key="colour-background">#36393e</Color>
    <Color x:Key="colour-background-dark">#1e2124</Color>
    <SolidColorBrush x:Key="background" Color="{StaticResource colour-background}"/>
    <SolidColorBrush x:Key="background-light" Color="{StaticResource colour-background-light}"/>
    <SolidColorBrush x:Key="background-dark" Color="{StaticResource colour-background-dark}"/>

    <Color x:Key="colour-main-light">#BEAEC2</Color>
    <Color x:Key="colour-main">#A188A6</Color>
    <Color x:Key="colour-main-dark">#86698C</Color>
    <SolidColorBrush x:Key="main" Color="{StaticResource colour-main}"/>
    <SolidColorBrush x:Key="main-light" Color="{StaticResource colour-main-light}"/>
    <SolidColorBrush x:Key="main-dark" Color="{StaticResource colour-main-dark}"/>

    <Color x:Key="colour-success-light">#00B8AE</Color>
    <Color x:Key="colour-success">#006C67</Color>
    <Color x:Key="colour-success-dark">#003D3A</Color>
    <SolidColorBrush x:Key="success-light" Color="{StaticResource colour-success-light}"/>
    <SolidColorBrush x:Key="success" Color="{StaticResource colour-success}"/>
    <SolidColorBrush x:Key="success-dark" Color="{StaticResource colour-success-dark}"/>


    <Color x:Key="colour-warning-light">#F4CE90</Color>
    <Color x:Key="colour-warning">#EDAE49</Color>
    <Color x:Key="colour-warning-dark">#E99C20</Color>
    <SolidColorBrush x:Key="warning-light" Color="{StaticResource colour-warning-light}"/>
    <SolidColorBrush x:Key="warning" Color="{StaticResource colour-warning}"/>
    <SolidColorBrush x:Key="warning-dark" Color="{StaticResource colour-warning-dark}"/>


    <Color x:Key="colour-failure-light">#EA9386</Color>
    <Color x:Key="colour-failure">#E26D5A</Color>
    <Color x:Key="colour-failure-dark">#DA462F</Color>
    <SolidColorBrush x:Key="failure-light" Color="{StaticResource colour-failure-light}"/>
    <SolidColorBrush x:Key="failure" Color="{StaticResource colour-failure}"/>
    <SolidColorBrush x:Key="failure-dark" Color="{StaticResource colour-failure-dark}"/>
</ResourceDictionary>